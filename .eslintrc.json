{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "new-cap": "off",
    "no-unused-vars": "warn",
    "comma-dangle": "off",
    "indent": "off"
  }
}
